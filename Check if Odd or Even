import java.util.*;
public class OddorEven {
    public static void oddorreven(int n) {
        int bitMask = 1 ;
        if(( n & bitMask) == 0 ){
            //even number
            System.out.println("even number");

        } else {
            System.out.println("odd number");
        }
    }
        
    
    public static void main(String[] args) {
        oddorreven(3);
        oddorreven(11);
        oddorreven(14);
     }

    
}
