import java.util.*;
public class CountSetBits {
    public static int countsetbits(int n){
        int count = 0;
        while (n>0){
            if((n&1)!=0){
                count++;//check our LSB
            }    
            n =   n>>1;
        } 
        return count ;
    }


public static void main(String[] args) {
    System.out.println(countsetbits(10));
    System.out.println(countsetbits(15));
}
}
